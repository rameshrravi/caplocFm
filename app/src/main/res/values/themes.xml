<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Caploc" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/purple_500</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>
    <style name="hello_">
        <item name="android:textSize">20dp</item>
        <item name="android:letterSpacing">0.02</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="shahin_alam">
        <item name="android:textSize">20dp</item>
        <item name="android:letterSpacing">0.01</item>
        <item name="android:textColor">#253141</item>
        <item name="fontFamily">@font/nunito_sans_bold</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="sample_text">
        <item name="android:textSize">15sp</item>
        <item name="android:letterSpacing">0.01</item>
        <item name="android:textColor">#0E1012</item>
    </style>

    <style name="lorem_ipsum">
        <item name="android:textSize">18sp</item>
        <item name="android:letterSpacing">0.01</item>
        <item name="fontFamily">@font/nunito_sans</item>
        <item name="android:textColor">#4A545E</item>
    </style>

    <style name="read_more">
        <item name="android:textSize">11sp</item>
        <item name="android:letterSpacing">0.01</item>
        <item name="android:textColor">#000000</item>

    </style>
    <style name="welcome_bac">
